{
  "expo": {
    "name": "Anchor",
    "slug": "anchor-v2",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/anchor-icon.jpg",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/anchor-icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A1D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anchor.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/anchor-icon.jpg",
        "backgroundColor": "#1A1A1D"
      },
      "package": "com.anchor.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@sentry/react-native/expo",
      "expo-secure-store"
    ]
  }
}
